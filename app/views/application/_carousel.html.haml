#carousel-example-generic.carousel.slide{"data-ride" => "carousel"}
  %ol.carousel-indicators
    - @app_configuration.landing_images.each_with_index do |image, index|
      %li{"data-slide-to" => "#{index}", "data-target" => "#carousel-example-generic", class: "#{index == 0 ? 'active' : nil}"}
  
  .carousel-inner{:role => "listbox"}
    - @app_configuration.landing_images.each_with_index do |image, index|
      .item{class: "#{index == 0 ? 'active' : nil}"}
        .carousel_image_holder
          =image_tag(image, class: 'img-responsive', alt: 'Fisioterapeutas colegiados en constante formaciÃ³n', style: 'max-height: 299px; width: auto;')
  
  %a.left.carousel-control{'data-slide' => 'prev', href: '#carousel-example-generic', role: 'button'}
    %span.fa.fa-angle-left{'aria-hidden' => 'true', style: 'margin-top:150%'}
    %span.sr-only Anterior
  %a.right.carousel-control{'data-slide' => 'next', href: '#carousel-example-generic', role: 'button'}
    %span.fa.fa-angle-right{'aria-hidden' => 'true', style: 'margin-top:150%'}
    %span.sr-only Siguiente